{
  "name": "apps-tapp",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "tapp",
  "author": "swoter <iswoter@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "ssr": "quasar dev -m ssr",
    "pwa": "quasar dev -m pwa",
    "android": "quasar dev -m cordova -T android",
    "ios": "quasar dev -m cordova -T ios",
    "build": "quasar build",
    "build:ssr": "quasar build -m ssr",
    "build:pwa": "quasar build -m pwa",
    "build:android": "quasar build -m cordova -T android",
    "build:ios": "quasar build -m cordova -T ios",
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "tester": "mocha ./test-api/index.js"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.21.1",
    "core-js": "^3.19.1",
    "dotenv": "^10.0.0",
    "mocha": "^9.1.3",
    "quasar": "^1.0.0",
    "vue-i18n": "^8.0.0"
  },
  "devDependencies": {
    "@quasar/app": "^2.0.0",
    "@quasar/quasar-app-extension-dotenv": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^7.7.0",
    "eslint-webpack-plugin": "^2.4.0"
  },
  "browserslist": [
    "ie >= 11",
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
